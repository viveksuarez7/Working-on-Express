<link rel ="stylesheet" href="viv.css">
<h1> The posts page </h1>

<% for(var i = 0; i< posts.length;i++){%>
<li>
    <%= posts[i].team %> - <strong><%= posts[i].Club %></strong>
    
</li>
<% } %>
<% posts.forEach(function(post){ %>
<li>
    <%= post.team %> - <strong><%= post.Club %></strong>
    
</li>

<% }) %>